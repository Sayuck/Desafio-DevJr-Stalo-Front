{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@hooks": ["hooks"],
      "@hooks/*": ["hooks/*"],
      "@contexts/*": ["contexts/*"],
      "@constants": ["constants"],
      "@constants/*": ["constants/*"],
      "@services": ["services"],
      "@services/*": ["services/*"],
      "@database/*": ["database/*"],
      "@lib/*": ["lib/*"],
      "@pages/*": ["pages/*"],
      "@templates/*": ["templates/*"],
      "@layouts/*": ["layouts/*"],
      "@styles/*": ["styles/*"],
      "@UI/*": ["UI/*"],
      "@utils": ["utils"],
      "@utils/*": ["utils/*"],
      "@public/*": ["../public/*"],
      "@routes": ["routes"],
      "@theme": ["theme"],
      "@theme/*": ["theme/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
